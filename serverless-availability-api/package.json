{
  "name": "serverless-listings-api",
  "version": "1.0.0",
  "description": "Listings API build with Serverless Framework",
  "scripts": {
    "dev": "serverless offline --stage dev --region us-east-1 --httpPort 4000",
    "deploy": "serverless deploy --stage prod",
    "deploy:dev": "serverless deploy --stage dev",
    "remove": "serverless remove --stage prod",
    "remove:dev": "serverless remove --stage dev"
  },
  "dependencies": {
    "express": "^4.17.1",
    "serverless-http": "^2.7.0",
    "serverless": "^3.22.0"
  },
  "devDependencies": {
    "serverless-offline": "^10.0.2"
  }
}
